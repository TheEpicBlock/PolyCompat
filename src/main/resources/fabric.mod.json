{
  "schemaVersion": 1,
  "id": "modid",
  "version": "${version}",

  "name": "Poly Compat",
  "description": "Companion to PolyMc, adds mod-specific code to aid serverside compatibility.",
  "authors": [
    "TheEpicBlock_TEB"
  ],
  "contact": {
    "homepage": "https://github.com/TheEpicBlock/PolyCompat/blob/master/README.md",
    "sources": "https://github.com/TheEpicBlock/PolyCompat"
  },

  "license": "CC0-1.0",
  "icon": "assets/modid/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "nl.theepicblock.polycompat.PolyCompat"
    ],
    "polymc": [
      "nl.theepicblock.polycompat.PolyCompat"
    ]
  },
  "mixins": [
    "polycompat.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.7.4",
    "fabric": "*",
    "minecraft": "1.16.x"
  },
  "suggests": {
    "flamingo": "*"
  }
}
